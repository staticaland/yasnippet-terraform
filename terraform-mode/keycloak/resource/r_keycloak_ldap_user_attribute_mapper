    ldap_attribute = "${2:ldap_attribute}"
    ldap_user_federation_id = "${3:ldap_user_federation_id}"
    name = "${4:name}"
    realm_id = "${5:realm_id}"
    user_model_attribute = "${6:user_model_attribute}"
}