    comp = "${2:comp}"
    email = "${3:email}"
    expire = "${4:expire}"
    name = "${5:name}"
    negative_cache = "${6:negative_cache}"
    refresh = "${7:refresh}"
    retry = "${8:retry}"
    serial = "${9:serial}"
    ttl = "${10:ttl}"
}