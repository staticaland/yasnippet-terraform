resource "civo_firewall_rule" "${1:this}" {
    cidr = "${2:cidr}"
    direction = "${3:direction}"
    end_port = "${4:end_port}"
    firewall_id = "${5:firewall_id}"
    protocol = "${6:protocol}"
    start_port = "${7:start_port}"
}